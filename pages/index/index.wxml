<view class="container">
  <view class="a-section a-brand">
    <view class="b">
      <view class="item item-1" wx:for="{{goods}}" wx:key="item.id">
          <view class="wrap">
            <view class="mt">
              <text class="brand">{{item.name}}</text>
              <text class="price">{{item.retail_price}}</text>
              <text class="unit">玩具</text>
            </view>
          </view>
      </view>
    </view>
  </view>
</view>